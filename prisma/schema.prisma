generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
    id            Int       @id @default(autoincrement())
    name          String?
    role          String    @default("user")
    position      String?
    deptChange   String?
    email         String    @unique
    emailVerified DateTime?
    image         String?
    createdAt     DateTime  @default(now())
    updatedAt     DateTime  @updatedAt
}

model SparePart {
  id          Int       @id @default(autoincrement())
  name        String
  description String?
  image       String?
  price       Float
  createdAt   DateTime  @default(now())
  // updatedAt   DateTime  @updatedAt
  updatedAt   DateTime  @default(now())
  cn0         Int
  cn1         Int
  cn2         Int
  cn3         Int
  cn4         Int
  cn5         Int
  cn6         Int
  cn7         Int
  cn8         Int
  cn9         Int
  cn10        Int
  cn11        Int
  cn12        Int
}

